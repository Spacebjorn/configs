# Realias vi+vim to neovim
alias vi=nvim
alias vim=nvim

alias info="info --vi-keys"

# git shorthands
alias gp="git push"
alias gpu='git push -u origin $(git branch --show-current)'
alias gpl="git pull"
alias gs="git status"
alias gco="git checkout"
alias gcob="git checkout -b"
alias gc="git commit"
alias ga="git add"
